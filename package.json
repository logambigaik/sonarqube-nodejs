{
	"scripts": {
		"sonar": "node sonar-project.js",
		"test": "jest --coverage"
	},
	"jest": {
		"testEnvironment": "node",
		"coveragePathIgnorePatterns": [
			"/node_modules/"
		],
		"testResultsProcessor": "jest-sonar-reporter"
	},
	"jestSonar": {
		"reportPath": "coverage",
		"reportFile": "test-reporter.xml",
		"indent": 4
	},
	"dependencies": {
		"express": "^4.17.1",
		"jest-junit": "^11.1.0"
	}
}
